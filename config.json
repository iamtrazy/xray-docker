{
  "log": {
    "disabled": true,
    "level": "info",
    "output": "/var/log/sing-box/access.log"
  },
  "inbounds": [
    {
      "type": "vless",
      "tag": "VLESS-WS",
      "listen": "/dev/shm/vws.sock",
      "tcp_fast_open": true,
      "users": [
        {
          "uuid": "$UUID"
        }
      ],
      "transport": {
        "type": "ws",
        "path": "/iamtrazy"
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ]
}
